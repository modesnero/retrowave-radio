{
  "name": "retrowave",
  "version": "0.1.0",
  "private": true,
  "license": "UNLICENSED",
  "author": "modesnero <modesnero@gmail.com>",
  "description": "retro wave radio",
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node app.js",
    "dev": "concurrently -c \"green.bold,red.bold\" \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon app.js",
    "client:install": "npm install --prefix client",
    "client:dev": "cross-env NODE_ENV=development npm run start --prefix client",
    "client:build": "npm run build --prefix client"
  },
  "keywords": [
    "retro",
    "retrowave",
    "synthwave",
    "radio",
    "music"
  ],
  "devDependencies": {
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.2.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "nodemon": "^2.0.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.1",
    "bcryptjs": "^2.4.3",
    "bootswatch": "^4.5.0",
    "config": "^3.3.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.19",
    "node-sass": "^4.14.1",
    "react-bootstrap": "^1.0.1"
  }
}
